{
  "name": "wishlist-importer",
  "version": "1.0.2",
  "description": "A wishlist importer library for providing the wishlist items data",
  "main": "dist/index.js",
  "types": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlGhaliyahM/wishlist-importer.git"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev": "nodemon",
    "start": "npm run build && node dist/index.js",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "husky install",
    "_postinstall": "husky install",
    "lintfix": "eslint --fix --ext \".ts,.js\" --ignore-path .eslintignore ."
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "npm run lintfix"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wishlist",
    "importer",
    "wishlist-scraper",
    "wishlist-importer"
  ],
  "author": "AlGhaliyah M.",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "^0.0.1-security",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "cheerio": "^1.0.0-rc.12"
  }
}
